{
  "sections": {
    "main": {
      "type": "main-product",
      "blocks": {
        "vendor": {
          "type": "text",
          "settings": {
            "content": "<p>{{ product.title }}<\/p>"
          }
        },
        "subtitle": {
          "type": "text",
          "settings": {
            "content": "<p>{{ product.vendor }}<\/p>"
          }
        },
        "bc4d7d4d-5f3e-4ee8-94d0-4ec9a855f14c": {
          "type": "variant_selector",
          "settings": {
            "selector_mode": "dropdown",
            "color_mode": "dropdown",
            "show_quantity_selector": true,
            "show_taxes_included": true,
            "show_inventory_quantity": true,
            "low_inventory_threshold": 4
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_payment_button": true
          }
        },
        "description": {
          "type": "description",
          "settings": {
            "display_mode": "show_all"
          }
        },
        "9de40edf-467c-45fe-a4ab-255051d7a377": {
          "type": "liquid",
          "settings": {
            "title": "Product title",
            "liquid": "{{ product.title }}",
            "display_mode": "collapse"
          }
        },
        "23dc10e5-c081-44c8-8fc4-1a4418a34f57": {
          "type": "liquid",
          "settings": {
            "title": "{{ product.vendor }}",
            "liquid": "{{ product.description }}",
            "display_mode": "collapse"
          }
        }
      },
      "block_order": [
        "vendor",
        "subtitle",
        "bc4d7d4d-5f3e-4ee8-94d0-4ec9a855f14c",
        "buy_buttons",
        "description",
        "9de40edf-467c-45fe-a4ab-255051d7a377",
        "23dc10e5-c081-44c8-8fc4-1a4418a34f57"
      ],
      "settings": {
        "image_size": "small",
        "enable_video_looping": false,
        "enable_image_zoom": true,
        "zoom_effect": "outside",
        "carousel_effect": "fade"
      }
    },
    "product-recommendations": {
      "type": "product-recommendations",
      "settings": {
        "heading": "You may also like",
        "show_quick_buy": false
      }
    }
  },
  "order": [
    "main",
    "product-recommendations"
  ]
}
