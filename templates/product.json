{
  "sections": {
    "main": {
      "type": "main-product",
      "blocks": {
        "vendor": {
          "type": "text",
          "settings": {
            "content": "<p>{{ product.title }}<\/p>"
          }
        },
        "subtitle": {
          "type": "text",
          "settings": {
            "content": "<p>{{ product.vendor }}<\/p>"
          }
        },
        "bc4d7d4d-5f3e-4ee8-94d0-4ec9a855f14c": {
          "type": "variant_selector",
          "settings": {
            "selector_mode": "dropdown",
            "color_mode": "dropdown",
            "show_quantity_selector": true,
            "show_taxes_included": true,
            "show_inventory_quantity": true,
            "low_inventory_threshold": 4
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_payment_button": true
          }
        },
        "description": {
          "type": "description",
          "disabled": true,
          "settings": {
            "display_mode": "collapse"
          }
        },
        "b2544c6e-901f-4db1-8510-1c770d5bedc2": {
          "type": "liquid",
          "settings": {
            "title": "Description",
            "liquid": "{{ product.description }}\n\n{% for image in product.images %}\n  <img src=\"{{ image.src | product_img_url: 'Large' }}\">\n{% endfor %}\n\n{% assign videos = product.media | where: \"media_type\", \"video\" %}\n{% for video in videos %}\n  {{ video.alt }}\n{% endfor %}",
            "display_mode": "collapse"
          }
        },
        "9de40edf-467c-45fe-a4ab-255051d7a377": {
          "type": "liquid",
          "settings": {
            "title": "Product title",
            "liquid": "{{ product.title }}",
            "display_mode": "collapse"
          }
        },
        "8497a996-a79c-4f99-96c6-6add7363245f": {
          "type": "text",
          "settings": {
            "content": ""
          }
        }
      },
      "block_order": [
        "vendor",
        "subtitle",
        "bc4d7d4d-5f3e-4ee8-94d0-4ec9a855f14c",
        "buy_buttons",
        "description",
        "b2544c6e-901f-4db1-8510-1c770d5bedc2",
        "9de40edf-467c-45fe-a4ab-255051d7a377",
        "8497a996-a79c-4f99-96c6-6add7363245f"
      ],
      "settings": {
        "image_size": "small",
        "enable_video_looping": false,
        "enable_image_zoom": true,
        "zoom_effect": "outside",
        "carousel_effect": "fade"
      }
    },
    "product-recommendations": {
      "type": "product-recommendations",
      "settings": {
        "heading": "You may also like",
        "show_quick_buy": false
      }
    }
  },
  "order": [
    "main",
    "product-recommendations"
  ]
}
